{
  "name": "tslint-jsx-typed-class-name",
  "version": "1.1.0",
  "description": "TSLint rule to provide typed class names for the jsx attribute className. Add more safety to your react components, especially with the css utility library of your choice",
  "keywords": [
    "tslint",
    "tslint-rule",
    "className",
    "jsx",
    "typed",
    "react",
    "tachyons-css",
    "tailwind-css",
    "css"
  ],
  "main": "tslint-jsx-typed-class-name.json",
  "author": "axol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelo/tslint-jsx-typed-class-name.git"
  },
  "scripts": {
    "build": "tsc --project ./",
    "copy": "cp -f package.json README.md tslint-jsx-typed-class-name.json build/",
    "clean": "rm -rf build/",
    "test": "yarn build && tslint -r build/ --test test/**/tslint.json"
  },
  "dependencies": {
    "chokidar": "^2.0.2",
    "css": "^2.2.1",
    "require-relative": "^0.8.7"
  },
  "peerDependencies": {
    "tslint": "^5.7.0",
    "typescript": "^2.4.0"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.4",
    "@types/css": "^0.0.31",
    "@types/node": "^8.9.4",
    "@types/require-relative": "^0.8.0",
    "chokidar": "^2.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
